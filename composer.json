{
    "name": "alt-three/segment",
    "description": "A Segment Bridge For Laravel 5",
    "keywords": ["segment", "bridge", "segment.com", "segment.io", "api", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/config": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "segmentio/analytics-php": "~1.5.2"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.1",
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Segment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Segment\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "laravel": {
            "providers": [
                "AltThree\\Segment\\SegmentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
