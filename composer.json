{
    "name": "alt-three/segment",
    "description": "A Segment Bridge For Laravel 5",
    "keywords": ["segment", "bridge", "segment.com", "segment.io", "api", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/config": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "segmentio/analytics-php": "1.3.*"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.0",
        "graham-campbell/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Segment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Segment\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "AltThree\\Segment\\SegmentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
